CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE CONTRACTDOCUMENT(CONTRACTDOCUMENTID VARCHAR(255) NOT NULL PRIMARY KEY,TRANSACTIONPRECENTFEE DOUBLE,CONTRACTDATE TIMESTAMP,MERCHANTID INTEGER)
CREATE MEMORY TABLE MERCHANT(MERCHANTACCOUNTID INTEGER NOT NULL PRIMARY KEY,NAME VARCHAR(255),RECEIPTS DOUBLE,FEEAMOUNT DOUBLE,CATEGORY INTEGER,STATUS INTEGER)
CREATE MEMORY TABLE PAYMENT(PAYMENTID VARCHAR(255) NOT NULL PRIMARY KEY,PAYINGACCOUNTID INTEGER,RECEIVINGMERCHANTID INTEGER,DESCRIPTION VARCHAR(255),PAYMENTAMOUNT DOUBLE,STATUS INTEGER,CREATEDDATE TIMESTAMP)
CREATE MEMORY TABLE PROCESSINGFEE(PROCESSINGFEEID VARCHAR(255) NOT NULL PRIMARY KEY,PAYINGACCOUNTID INTEGER,DEPENDENTPAYMENTID VARCHAR(255),DESCRIPTION VARCHAR(255),AMOUNT DOUBLE,STATUS INTEGER,CREATEDDATE TIMESTAMP)
CREATE MEMORY TABLE USER(USERACCOUNTID INTEGER NOT NULL PRIMARY KEY,NAME VARCHAR(255),BALANCE DOUBLE,CREDITLIMIT DOUBLE,STATUS INTEGER)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
